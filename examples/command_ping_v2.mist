command ping (ip) {
    exec "ping -c 1 {ip}" {
        input {
            ip <= ip
        }
        then {
            print "exec result is" result
            expose result
            expose console
        }
    } as miexecution
    expose ip
    expose result <= miexecution.result
    expose console <= miexecution.console
}

call ping {
    input {
        ip <= "127.0.0.1"
    }
    then {
        print "ping result is" result
    }
}
