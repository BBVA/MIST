# Name: exec
# Description: executes a custom user shell command
# Tags: command, exec, shell

exec "bash -c ls {hidden} -1 {filter}" {
    input {
        filter = "*"
        hidden = "-a"
    }
    output {
        result
        resultCode
        consoleOutput
        consoleError
    }
    then {
        print resultCode
    }
}
