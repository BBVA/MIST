PythonCodeAnalysis {
    input {
        sources <= %sourcesPath
    }
    output {
        hasVulnerabilities
        issues
        result
        console
    }
    then {
        _load_mist_model_ result is Error {
            Abort "Issues detected!"
        }
        _load_mist_model_ result is Success {
            print "Code is clean and safe"
        }
    }
}
