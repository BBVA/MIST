BuiltIn:
    BuiltPrint | BuiltExec | BuiltSearchInText | BuiltSearchInXML
;

BuiltPrint:
    'print' text=IDorSTRING
;

Param:
    key=ID '=' value=STRING
;

BuiltExec:
  'exec' command=STRING '{'
    'input' '{'
      params+=Param
    '}'
    ('output' '{'
      'result'
      'resultCode'
      'consoleOutput'
      'consoleError'
    '}')?
    'then' '{'
      commands*=Command
    '}'
  '}'
;

BuiltSearchInText:
    'searchInText' regex=STRING text=IDorSTRING '{'
    ('output' '{'
      'result'
      'found'
    '}')?
    'then' '{'
      commands*=Command
    '}'
  '}'
;

BuiltSearchInXML:
    'searchInText' xpath=STRING text=IDorSTRING '{'
    ('output' '{'
      'result'
      'found'
      'value'
    '}')?
    'then' '{'
      commands*=Command
    '}'
  '}'
;
 