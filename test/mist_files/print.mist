data myTable {
    ip
    result
}

ping {
    input {
        ip <= "127.0.0.1"
    }
    then {
        print "The result of ping to {ip} is {result}"
        put ip result => myTable
    }
}

iterate myTable => t {
    print t.ip
    print "Saved result of ping to {t.ip} is {t.result}"
}

print "myTable contains {myTable}"
