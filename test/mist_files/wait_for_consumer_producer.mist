function customSend(n) {
    print("Begin send {n}")
    send n
    print("End send {n}")
}

function passNumber(n,t) {
    print("begin passNumber {n} {t}")
    sleep(ms=t)
    send n
    print("end passNumber {n} {t}")
}

function printNumber(n) {
    print("printNumber {n}")
}

customSend(1) => p
passNumber(:p,3) => p2
passNumber(:p,1) => p2
printNumber(:p2)

