exec "bash -c ls {hidden} -1 {filter}" {
    input {
        filter <= "*"
        hidden <= "-a"
    }
    output {
        result
        resultCode
        consoleOutput
        consoleError
    }
    then {
        print "First exec result is {result}"
    }
}

exec "echo {message}" {
    input {
        message <= %param
    }
    then {
        print "Second exec result is {result} and console output is {consoleOutput}"
    }
}

