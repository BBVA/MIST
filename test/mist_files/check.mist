# Name: check
# Version: 1.0.0
# Description: executes a branch or not based on check result using 'is' operator
# Tags: command, check

exec "/bin/bash -c {command}" {
  input {
    command <= %param
  }
  output {
    result
    resultCode
    consoleOutput
    consoleError
  }
  then {
    check result is Success {
      print "FOO"
    }
  }
}
