# Name: CSV
# Description: Read a CSV file and write another
# Tags:  csv

CSVput "test/myHosts.csv" => myHosts

data myNewHosts {
    ip
    so
    status
}

iterate myHosts => host {
    put host.ip host.so "up" => myNewHosts
}

CSVdump myNewHosts => "test/myNewHosts.csv"

CSVput "test/myNewHosts.csv" => myNewReadedHosts

print myNewReadedHosts
